{
  "name": "citypantry-error-plugin",
  "author": "Lewis Wilcock <lewis.wilcock@gmail.com>, Paul Lessing <paul@paullessing.com>",
  "license": "MIT",
  "version": "1.7.0",
  "description": "Error reporting chrome plugin for citypantry.com",
  "main": "serverless.ts",
  "scripts": {
    "serve": "concurrently \"yarn serve:background\" \"yarn serve:popup\" \"yarn serve:content\" -n \"backg,popup,conte\"",
    "serve:background": "webpack --config webpack.background.config.js --watch --progress --mode development",
    "serve:popup": "webpack --config webpack.popup.config.ts --watch --progress --mode development",
    "serve:content": "webpack --config webpack.content.config.js --watch --progress --mode development",
    "build": "yarn build:background && yarn build:popup && yarn build:content",
    "build:background": "webpack --config webpack.background.config.js --mode production --progress",
    "build:popup": "webpack --config webpack.popup.config.ts --mode production --progress",
    "build:content": "webpack --config webpack.content.config.js --mode production --progress",
    "deploy": "sls deploy -f report"
  },
  "dependencies": {
    "aws-sdk": "^2.209.0",
    "axios": "^0.21.1",
    "form-data": "^3.0.0",
    "pify": "^3.0.0",
    "preact": "^8.4.2",
    "source-map-support": "^0.5.19",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@serverless/typescript": "^2.16.0",
    "@types/aws-lambda": "^8.10.64",
    "@types/chrome": "^0.0.60",
    "@types/mini-css-extract-plugin": "^0.9.0",
    "@types/node": "^14.14.7",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/terser-webpack-plugin": "^4.2.0",
    "@types/uuid": "^3.4.5",
    "@types/webpack": "^4.41.25",
    "@types/webpack-dev-server": "^3.11.1",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "concurrently": "^3.5.1",
    "css-loader": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.1",
    "serverless": "^2.16.0",
    "serverless-webpack": "^5.2.0",
    "style-loader": "^0.20.3",
    "svelte": "^3.29.7",
    "svelte-check": "^1.1.15",
    "svelte-loader-hot": "^0.3.1",
    "svelte-preprocess": "^4.6.1",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^8.0.10",
    "ts-node": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.2",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.2"
  }
}
