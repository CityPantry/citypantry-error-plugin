{
  "name": "CP Error Reporting",
  "short_name": "CP Error Reporting",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Easily report errors on citypantry.com",
  "homepage_url": "http://info.citypantry.com/careers",
  "browser_action": {
    "default_title": "Report an Error",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://citypantry.com/*",
        "https://*.citypantry.com/*",
        "https://*.c8y.tech/*"
      ],
      "js": ["js/content.js"]
    }
  ],
  "web_accessible_resources": ["/js/content.js"],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "64": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "tabs",
    "identity",
    "https://ingfo0ccaa.execute-api.eu-west-2.amazonaws.com/*",
    "https://www.googleapis.com/oauth2/v2/userinfo",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "oauth2": {
    "client_id": "569754864639-m4qg4jo8gnnucbsa9nsomo49c9oqh5ii.apps.googleusercontent.com",
    "scopes":[
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  },
  "key": ""
}
